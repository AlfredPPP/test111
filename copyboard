import re

# 你的文本样例（可以从文件读取或者直接输入）
text = """
Management fees 16 1,634 1,919
Custody fees 21 36
Total investment income/(loss) 30,267 33,999
Other income - 4
"""

# 正则表达式匹配管理费用、保管费用等
pattern = r'([A-Za-z/\(\)\s]+?)\s+([-]?\d+[\d,]*)\s+([-]?\d+[\d,]*)\s*([-]?\d+[\d,]*)?'

matches = re.findall(pattern, text)

# 格式化输出
results = []
for match in matches:
    result = [item for item in match if item]
    results.append(result)

# 输出结果
for result in results:
    print(result)