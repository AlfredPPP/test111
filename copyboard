import win32com.client

def get_excel_last_saved_date(file_path):
    excel = win32com.client.Dispatch("Excel.Application")
    workbook = excel.Workbooks.Open(file_path)

    last_saved_date = workbook.BuiltinDocumentProperties("Last Save Time")
    
    workbook.Close(SaveChanges=False)
    excel.Quit()

    return last_saved_date

file_path = r"path\to\your\file.xlsx"  # 替换为你的Excel文件路径
last_saved_date = get_excel_last_saved_date(file_path)
print(f"Date last saved: {last_saved_date}")