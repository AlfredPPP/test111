import re

def find_all_altTexts_before_aaaaas(text):
    # 找出所有 altText 的位置和值
    alt_matches = list(re.finditer(r'"altText"\s*:\s*"([^"]+)"', text))

    # 找出所有 aaaaa 的位置
    aaaaa_matches = list(re.finditer(r'aaaaa', text))

    if not alt_matches or not aaaaa_matches:
        return []

    results = []

    for aaaaa_match in aaaaa_matches:
        aaaaa_pos = aaaaa_match.start()

        # 找出所有在当前 aaaaa 之前的 altText
        alt_before = [m for m in alt_matches if m.start() < aaaaa_pos]
        if alt_before:
            nearest_alt = alt_before[-1]  # 最后一个是最近的
            alt_value = nearest_alt.group(1)
            results.append(alt_value)
        else:
            results.append(None)  # 如果找不到就用 None 填充

    return results